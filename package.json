{
  "name": "@konstant/payload-plugin-navigation",
  "version": "0.2.7",
  "description": "Navigation plugin for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "dev": "payload run ./dev/server.ts",
    "dev:generate-importmap": "pnpm dev:payload generate:importmap",
    "dev:generate-types": "pnpm dev:payload generate:types",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=./dev/payload.config.ts payload",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "jest",
    "ci:version": "node scripts/ci-version.js",
    "ci:tag": "node scripts/tag.js",
    "ci:patch": "pnpm ci:version patch && pnpm ci:tag"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "import": "./src/exports/client.ts",
      "types": "./src/exports/client.ts",
      "default": "./src/exports/client.ts"
    },
    "./rsc": {
      "import": "./src/exports/rsc.ts",
      "types": "./src/exports/rsc.ts",
      "default": "./src/exports/rsc.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@payloadcms/plugin-nested-docs": "3.32.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@payloadcms/db-mongodb": "3.32.0",
    "@payloadcms/db-postgres": "3.32.0",
    "@payloadcms/db-sqlite": "3.32.0",
    "@payloadcms/eslint-config": "3.28.0",
    "@payloadcms/next": "3.32.0",
    "@payloadcms/richtext-lexical": "3.32.0",
    "@payloadcms/ui": "3.32.0",
    "@swc-node/register": "1.10.10",
    "@swc/cli": "0.6.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "child_process": "^1.0.2",
    "copyfiles": "2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.4",
    "fs": "0.0.1-security",
    "graphql": "^16.8.1",
    "jest": "29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "next": "15.2.4",
    "open": "^10.1.0",
    "payload": "3.32.0",
    "prettier": "^3.5.3",
    "qs-esm": "7.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rimraf": "^6.0.1",
    "semver": "^7.7.1",
    "sharp": "0.33.5",
    "sort-package-json": "^3.0.0",
    "typescript": "^5.8.2",
    "url": "^0.11.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
